{
    "name": "nativescript-iap",
    "version": "7.0.0",
    "description": "A NativeScript plugin for making in-app purchases",
    "repository": {
        "type": "git",
        "url": "https://github.com/PackagedCat/nativescript-iap"
    },
    "scripts": {
        "clean": "git clean -f -X -d",
        "build": "tsc -b -v && cd ./src && cpy ./**/*.d.ts ../package/ --parents",
        "setup": "npm run clean && npm install && ts-patch install"
    },
    "keywords": [
        "NativeScript",
        "iap",
        "in-app purchase",
        "in-app billing",
        "payment",
        "purchase",
        "billing",
        "subscription"
    ],
    "author": "Shamsuddin Dzhamalov <packagedcat@outlook.com>",
    "contributors": [],
    "license": "Apache-2.0",
    "devDependencies": {
        "@nativescript/core": "^7.3.0",
        "@nativescript/types": "^7.3.0",
        "@nativescript/webpack": "^3.0.9",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "cpy-cli": "^3.1.1",
        "eslint": "^7.22.0",
        "ts-patch": "^1.3.0",
        "typescript": "^3.9.9"
    },
    "dependencies": {
        "ts-node": "^9.0.0"
    }
}
